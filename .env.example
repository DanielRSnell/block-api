# Server Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_VERSION=v1
MAX_REQUEST_SIZE=10mb
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Security
CORS_ORIGIN=*
HELMET_ENABLED=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# Performance
ENABLE_COMPRESSION=true
CACHE_TTL=300

# Provider Configuration
ENABLE_GENERATEBLOCKS_PRO=true
ENABLE_CUSTOM_ELEMENTS=true
DEFAULT_PRESERVE_CLASSES=true
DEFAULT_GENERATE_UNIQUE_IDS=true